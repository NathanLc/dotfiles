alias ..='cd ..'
alias ...='cd ../..'
alias ls='ls -G'
alias c='clear'
alias cl='cd && clear'
alias dc='docker-compose'
alias ec='emacsclient -nc'
alias ll='ls -AlFG'
alias lt='ls -AlFtrG'
alias lt1='ls -FtrG | tail -1'
alias cat1='cat $(lt1)'
alias less1='less -S $(lt1)'
alias tmuxat='tmux a -t'
alias psg='ps aux | rg'
